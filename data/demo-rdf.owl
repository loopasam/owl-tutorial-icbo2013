    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.example.org/demo.owl#City -->

    <owl:Class rdf:about="&demo;City"/>
    


    <!-- http://www.example.org/demo.owl#Person -->

    <owl:Class rdf:about="&demo;Person">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
    </owl:Class>
    


    <!-- http://www.example.org/demo.owl#Scientist -->

    <owl:Class rdf:about="&demo;Scientist">
        <rdfs:subClassOf rdf:resource="&demo;Person"/>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.example.org/demo.owl#John -->

    <owl:NamedIndividual rdf:about="&demo;John">
        <rdf:type rdf:resource="&demo;Scientist"/>
        <works-in rdf:resource="&demo;Paris"/>
    </owl:NamedIndividual>
    


    <!-- http://www.example.org/demo.owl#Paris -->

    <owl:NamedIndividual rdf:about="&demo;Paris">
        <rdf:type rdf:resource="&demo;City"/>
    </owl:NamedIndividual>
</rdf:RDF>



